       "      <td>65788</td>\n",
       "      <td>102</td>\n",
       "      <td>0.16</td>\n",
       "    </tr>\n",
       "  </tbody>\n",
       "</table>\n",
       "</div>"
      ],
      "text/plain": [
       "  amount_bucket  total_transactions  fraud_count  fraud_rate_pct\n",
       "0         $500+                9492           35            0.37\n",
       "1     $200–$500               19823           50            0.25\n",
       "2     Under $50              189704          305            0.16\n",
       "3      $50–$200               65788          102            0.16"
      ]
     },
     "execution_count": 19,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "query = \"\"\"\n",
    "SELECT\n",
    "    CASE\n",
    "        WHEN Amount < 50 THEN 'Under $50'\n",
    "        WHEN Amount < 200 THEN '$50–$200'\n",
    "        WHEN Amount < 500 THEN '$200–$500'\n",
    "        ELSE '$500+'\n",
    "    END AS amount_bucket,\n",
    "    COUNT(*) AS total_transactions,\n",
    "    SUM(Class) AS fraud_count,\n",
    "    ROUND(100.0 * SUM(Class) / COUNT(*), 2) AS fraud_rate_pct\n",
    "FROM transactions\n",
    "GROUP BY amount_bucket\n",
    "ORDER BY fraud_rate_pct DESC;\n",
    "\"\"\"\n",
    "\n",
    "fraud_by_amount = pd.read_sql(query, conn)\n",
    "fraud_by_amount\n"
   ]
